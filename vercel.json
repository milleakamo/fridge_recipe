{
  "version": 2,
  "framework": "flutter",
  "installCommand": "flutter pub get",
  "buildCommand": "flutter build web --release",
  "outputDirectory": "build/web",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/((?!api/|assets/|canvaskit/).*)", "destination": "/index.html" }
  ]
}
